<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="crm_lost_reason_view_form" model="ir.ui.view">
        <field name="name">Travel form</field>
        <field name="model">member.list</field>
        <field name="arch" type="xml">
            <search string="Tests">
                <field name="id"/>
                <field name="create_uid"/>
            </search>
            <form string="Test">
                <sheet>
                    <group>
                        <group>
                            <field name="id"/>
                        </group>
                        <group>
                            <field name="create_uid"/>
                        </group>
                        <notebook>
                            <page string="Description">
                                <field name="write_uid"/>
                            </page>
                        </notebook>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="crm_lost_reason_view_tree" model="ir.ui.view">
        <field name="name">Travel tree</field>
        <field name="model">member.list</field>
        <field name="arch" type="xml">
            <tree string="Channel">
                <field name="id"/>
            </tree>
        </field>
    </record>
</odoo>
